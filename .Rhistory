xaxis = list(title = "Retention Time (s)"),
yaxis = list(title = "Intensity")
)
})
output$spectrumPlot <- renderPlotly({
click <- event_data("plotly_click", source = "bpc")
req(click, spectra_obj())
clicked_rt <- click$x
sp <- spectra_obj()
rt <- rtime(sp)
idx <- which.min(abs(rt - clicked_rt))
spec <- sp[idx]
plot_ly(
x = mz(spec)[[1]],
y = intensity(spec)[[1]],
type = 'scatter',
mode = 'lines'
) %>% layout(
title = paste("Mass Spectrum at RT =", round(clicked_rt, 2), "s"),
xaxis = list(title = "m/z"),
yaxis = list(title = "Intensity")
)
})
}
